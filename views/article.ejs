<%- include('header'); -%>
    <div class="content">
        <h1>Selected articles</h1>
        <table id="catalogue">
            <thead>
                <tr>
                    <th scope="col">Reference</th>
                    <th scope="col">Category</th>
                    <th scope="col">Name</th>
                    <th scope="col">Supplier</th>
                    <th scope="col">Price</th>
                    <th scope="col">VAT</th>
                    <th scope="col">Stock</th>
                    <th scope="col">Critical_Stock</th>

                </tr>
            </thead>
            <tbody>
                
                    <tr>
                        <td>
                            <%= products.reference %>
                        </td>
                        <td>
                            <%= products.category %>
                        </td>
                        <td>
                            <%= products.name %>
                        </td>
                        <td>
                            <%= products.supplier %>
                        </td>
                        <td>
                            <%= products.price %>
                        </td>
                        <td>
                            <%= products.vat %>
                        </td>
                        <td>
                            <%= products.stock %>
                        </td>
                        <td>
                            <%= products.critical_stock %>
                        </td>
                    </tr>
            </tbody>
        </table>
    </div>
    <br>
    <%- include('footer'); -%>